class Test{
	public static void main(String[] args){
		int[] arr = {1, 2, 4, 7, 7, 5};
		int ans=findSecondLarge(arr,arr.length);
		System.out.println(ans);
	}
	static int findSecondLarge(int arr[],int n){
		int large=Integer.MIN_VALUE;
		int sLarge=Integer.MIN_VALUE;
		for(int i=0;i<n;i++){
			if(arr[i]>large){
				sLarge=large;
				large=arr[i];
			}else if(arr[i]>sLarge && large!=arr[i]){
				sLarge=arr[i];
			}
		}
		return sLarge;
	}
}
